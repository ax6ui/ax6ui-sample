<!DOCTYPE html>
<html lang="ko">
    <head>
        <meta charset="utf-8" />
        <title><%= htmlWebpackPlugin.options.title || 'Webpack App'%></title>

        <% if (htmlWebpackPlugin.files.favicon) { %>
        <link rel="shortcut icon" href="<%= htmlWebpackPlugin.files.favicon%>" />
        <% } %>
        <meta name="viewport" content="width=device-width, initial-scale=1" />

        <% for (key in htmlWebpackPlugin.options.commonChunks) { %>
        <script src="<%= htmlWebpackPlugin.options.commonChunks[key] %>" type="text/javascript"></script>
        <% } %>

        <% for (key in htmlWebpackPlugin.files.chunks) { %>
        <script src="<%= htmlWebpackPlugin.files.chunks[key].entry %>" type="text/javascript"></script>
        <% } %>
    </head>
    <body>

        <main data-ax6ui-sidenav-panel="">
            <nav>
                <div class="nav-wrapper">
                    <div class="container">
                    <ul class="left" data-nav-account="">
                        <li><a href="#!" data-activates="slide-out" class="waves-effect waves-light"><i class="large material-icons">menu</i></a></li>
                    </ul>
                    <a href="#!" class="brand-logo" style="padding:0 20px;">AX6UI</a>
                    </div>
                </div>
            </nav>

            <div class="container">
                <div id="sample-body" style="padding-top: 1em;"></div>
            </div>
        </main>

        <div data-ax6ui-sidenav-menu="">
            <div class="sidenav">
            <ul>
                <% for (key in htmlWebpackPlugin.options.menu) { %>
                <li><a style="cursor:pointer;" data-href="<%= htmlWebpackPlugin.options.menu[key] %>"><%= htmlWebpackPlugin.options.menu[key] %></a></li>
                <% } %>
            </ul>
            </div>
        </div>

    </body>
</html>